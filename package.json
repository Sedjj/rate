{
	"name": "rate",
	"version": "1.0.0",
	"description": "Simple bot for rates",
	"main": "src/index.js",
	"scripts": {
		"start": "set NODE_ENV=production&&node src/index.js",
		"dev": "set NODE_ENV=development&&node src/index.js",
		"docs": "jsdoc -c jsdoc.json README.md",
		"help-api": "apidoc -i routes -o help/api",
		"build": "webpack --mode=development --config ./webpack.config.js --progress --profile",
		"test": "jest"
	},
	"repository": {
		"type": "git",
		"url": "git@gitlab.com:developmentKit/projects/bot/rate.git"
	},
	"keywords": [
		"telegram",
		"bot",
		"rate"
	],
	"jest": {
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	},
	"author": "Eldar Salimzebarov",
	"license": "MIT",
	"homepage": "https://gitlab.com:developmentKit/projects/bot/rate.git#readme",
	"dependencies": {
		"apidoc": "^0.17.7",
		"config": "^3.0.1",
		"cron": "^1.7.0",
		"form-data": "^2.3.3",
		"mongodb": "^3.1.13",
		"mongoose": "^5.4.19",
		"node-telegram-bot-api": "^0.30.0",
		"node-telegram-bot-api-middleware": "^0.1.3",
		"request": "^2.88.0",
		"socks5-https-client": "^1.2.1",
		"superagent": "^5.0.2",
		"winston": "^3.2.1",
		"winston-transport": "^4.3.0",
		"xlsx-template": "^0.5.0",
		"zip-local": "^0.3.4"
	},
	"devDependencies": {
		"eslint": "^5.15.1",
		"jsdoc": "^3.5.5",
		"resolve-path": "^1.4.0",
		"ts-loader": "^5.3.3",
		"tslint": "^5.14.0",
		"tslint-loader": "^3.5.4",
		"webpack": "^4.29.6",
		"webpack-cli": "^3.2.3",
		"webpack-node-externals": "^1.7.2"
	}
}
