{
	"name": "rate",
	"version": "1.0.0",
	"description": "Simple bot for rates",
	"main": "src/index.js",
	"scripts": {
		"start": "set NODE_ENV=production&&node src/index.js",
		"dev": "set NODE_ENV=development&&node src/index.js",
		"docs": "jsdoc -c jsdoc.json README.md",
		"help-api": "apidoc -i routes -o help/api",
		"build": "webpack --mode=development --config ./webpack.config.js --progress --profile",
		"test": "jest"
	},
	"repository": {
		"type": "git",
		"url": "git@gitlab.com:developmentKit/projects/bot/rate.git"
	},
	"keywords": [
		"telegram",
		"bot",
		"rate"
	],
	"prettier": {
		"singleQuote": true,
		"bracketSpacing": true,
		"trailingComma": "none"
	},
	"jest": {
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	},
	"author": "Eldar Salimzebarov",
	"license": "MIT",
	"homepage": "https://gitlab.com:developmentKit/projects/bot/rate.git#readme",
	"dependencies": {
		"apidoc": "0.17.7",
		"config": "3.1.0",
		"cron": "1.7.0",
		"got": "^9.6.0",
		"mongodb": "3.2.3",
		"mongoose": "5.5.2",
		"node-telegram-bot-api": "0.30.0",
		"node-telegram-bot-api-middleware": "0.1.3",
		"request": "2.88.0",
		"request-cookies": "1.1.0",
		"socks5-https-client": "1.2.1",
		"tough-cookie": "^3.0.1",
		"winston": "3.2.1",
		"xlsx-template": "0.5.0",
		"zip-local": "0.3.4"
	},
	"devDependencies": {
		"eslint": "5.16.0",
		"jsdoc": "3.6.2",
		"resolve-path": "1.4.0",
		"ts-loader": "5.3.3",
		"tslint": "5.15.0",
		"tslint-loader": "3.5.4",
		"webpack": "4.30.0",
		"webpack-cli": "3.3.0",
		"webpack-node-externals": "1.7.2"
	}
}
